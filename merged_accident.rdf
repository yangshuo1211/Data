    <ns1:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Explosion at a fireworks factory</ns1:hasTitle>
    <ns1:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-06-24T13:00:00</ns1:hasStartDate>
    <ns1:hasFinishDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-06-24T13:00:00</ns1:hasFinishDate>
    <ns1:hasLegislation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EU Seveso II Directive</ns1:hasLegislation>
    <ns1:hasEventType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Major Accident</ns1:hasEventType>
    <ns1:hasEmergencyResponse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The operator launched the internal emergency plan and evacuated the 160 staff.</ns1:hasEmergencyResponse>
    <ns1:hasLessonsLearned rdf:datatype="http://www.w3.org/2001/XMLSchema#string">For explosive products, attention must be paid to oxidants and fuels coming into contact.</ns1:hasLessonsLearned>
