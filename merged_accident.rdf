  <rdf:Description rdf:about="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident_458">
    <rdf:type rdf:resource="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident"/>
    <ns1:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fire in industry for metal processing</ns1:hasTitle>
    <ns1:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">1995-07-15T22:45:00</ns1:hasStartDate>
