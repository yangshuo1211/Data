  <rdf:Description rdf:about="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident_97">
    <rdf:type rdf:resource="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident"/>
    <ns1:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">15 April 2016 Fire and substance release at a blast furnace of a smelting works</ns1:hasTitle>
    <ns1:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-04-15T21:00:00</ns1:hasStartDate>
    <ns1:hasFinishDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-04-15T00:00:00</ns1:hasFinishDate>
    <ns1:hasLegislation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EU Seveso II Directive</ns1:hasLegislation>
    <ns1:hasEventType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Major Accident</ns1:hasEventType>
    <ns1:hasEmergencyResponse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Firefighting efforts by the fire service.Blast furnace cut off from the gas network.On-site fire service assisted by the fire brigade.</ns1:hasEmergencyResponse>
    <ns1:hasLessonsLearned rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It will remain the case that jet fires cannot be definitively ruled out. Nonetheless, measures are currently being tested to further reduce the effects of jet fires (see limitation safeguards). — Exchange of experience with other blast furnace operators.- Assess the ability to retrofit firefighting systems around cable runs.</ns1:hasLessonsLearned>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident_43">
    <rdf:type rdf:resource="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident"/>
    <ns1:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operators burnt after wrong manual operation on naphta cracker oven</ns1:hasTitle>
    <ns1:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2005-05-04T20:00:00</ns1:hasStartDate>
    <ns1:hasFinishDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2005-05-04T00:00:00</ns1:hasFinishDate>
