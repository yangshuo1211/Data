  <rdf:Description rdf:about="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident_1304">
    <rdf:type rdf:resource="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident"/>
    <ns1:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Explosion of hexane vapour in a rapeseed oil extractor</ns1:hasTitle>
    <ns1:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-07-05T21:05:00</ns1:hasStartDate>
    <ns1:hasFinishDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-07-12T00:00:00</ns1:hasFinishDate>
    <ns1:hasLegislation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EU Seveso III Directive</ns1:hasLegislation>
    <ns1:hasEventType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Major Accident</ns1:hasEventType>
    <ns1:hasEmergencyResponse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The extensive fire was extinguished over 7 days.&amp;nbsp;</ns1:hasEmergencyResponse>
    <ns1:hasLessonsLearned rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A comprehensive HAZOP study would be conducted before the unit come into service.&amp;nbsp;</ns1:hasLessonsLearned>
