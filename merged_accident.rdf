    <ns1:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Explosion during welding works of a pipe </ns1:hasTitle>
    <ns1:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">1993-07-01T12:00:00</ns1:hasStartDate>
    <ns1:hasFinishDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">1993-07-01T00:00:00</ns1:hasFinishDate>
    <ns1:hasLegislation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EU Seveso I Directive</ns1:hasLegislation>
    <ns1:hasEventType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Major Accident</ns1:hasEventType>
    <ns1:hasEmergencyResponse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">not given</ns1:hasEmergencyResponse>
    <ns1:hasLessonsLearned rdf:datatype="http://www.w3.org/2001/XMLSchema#string">For future assembly works a written procedure will be prepared in order to avoid misunderstandings (written procedure missing).</ns1:hasLessonsLearned>
  </rdf:Description>
