  <rdf:Description rdf:about="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident_997">
    <rdf:type rdf:resource="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident"/>
    <ns1:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Explosion in an ammunition neutralizing plant</ns1:hasTitle>
    <ns1:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-01-01T00:00:00</ns1:hasStartDate>
    <ns1:hasFinishDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-01-01T00:00:00</ns1:hasFinishDate>
    <ns1:hasLegislation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EU Seveso II Directive</ns1:hasLegislation>
    <ns1:hasEventType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Major Accident</ns1:hasEventType>
    <ns1:hasEmergencyResponse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cordoning of the affected plant area.Call for external emergency response services.DecontaminationAcid neutralisation performed by the fire brigade.</ns1:hasEmergencyResponse>
    <ns1:hasLessonsLearned rdf:datatype="http://www.w3.org/2001/XMLSchema#string">nan</ns1:hasLessonsLearned>
