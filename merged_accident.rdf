    <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-01-19T19:57:35+00:00</dc:date>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.semanticweb.org/shuoyang/ontologies/Accident#DateTime">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
