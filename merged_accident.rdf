  <rdf:Description rdf:about="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident_296">
    <rdf:type rdf:resource="http://www.semanticweb.org/shuoyang/ontologies/Accident#Accident"/>
    <ns1:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Release of NH3 gas during start up of Urea production unit</ns1:hasTitle>
    <ns1:hasStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-04-27T20:00:00</ns1:hasStartDate>
    <ns1:hasFinishDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-04-28T02:00:00</ns1:hasFinishDate>
    <ns1:hasLegislation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EU Seveso II Directive</ns1:hasLegislation>
    <ns1:hasEventType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Major Accident</ns1:hasEventType>
    <ns1:hasEmergencyResponse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">6 hours after the emission started, the installation was shut down, thereby ending the emission.</ns1:hasEmergencyResponse>
    <ns1:hasLessonsLearned rdf:datatype="http://www.w3.org/2001/XMLSchema#string">- Quicker response to complaints
- Possible improvement of maintenance (timely replacement of packages)
- Immediate reporting to authorities</ns1:hasLessonsLearned>
