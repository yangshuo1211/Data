    <ns1:hasFinishDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">1988-03-03T00:00:00</ns1:hasFinishDate>
    <ns1:hasLegislation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EU Seveso I Directive</ns1:hasLegislation>
    <ns1:hasEventType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Major Accident</ns1:hasEventType>
    <ns1:hasEmergencyResponse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">INTERNAL TO THE ESTABLISHMENT:The explosives production was halted.No off-site emergency measures were necessary (code 7703). No emergency measures are still required, neither on-site nor off-site (code 7703).</ns1:hasEmergencyResponse>
    <ns1:hasLessonsLearned rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Not given</ns1:hasLessonsLearned>
